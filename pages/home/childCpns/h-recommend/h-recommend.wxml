<!--pages/home/childCpns/h-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{recommends}}" wx:key="index">
    <view class="recommend-item">
      <image src="{{item.image}}" class="recommend-image" bind:load="handerImageLoad" />
      <view>{{item.title}}</view>
    </view>
  </block>
</view>
